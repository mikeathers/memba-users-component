  "bin": {
    "memba-users-component": "src/memba-users-component.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "prepare": "husky install",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "prettier": "prettier \"./src/**/*.{ts,js,json}\" --check",
    "prettier:fix": "prettier \"./src/**/*.{ts,js,json}\" --check --write",
    "lint": "eslint . --fix --ext ts,tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "yarn test --coverage",
    "test:badges": "yarn test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/aws-lambda": "^8.10.109",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "20.1.7",
    "@types/prettier": "2.6.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "esbuild": "^0.15.12",
    "eslint": "^7.6.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.27.4",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-acm": "^3.348.0",
    "@aws-sdk/client-route-53": "^3.344.0",
    "aws-cdk": "2.80.0",
    "aws-cdk-lib": "2.80.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1394.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
